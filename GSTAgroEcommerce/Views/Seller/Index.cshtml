<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script>
    $(document).ready(function () {
        var month = [];
        var monthdata = [];
        //var sid = $(this).val();
        // $("#CityId").empty();
        $.get("Seller/ShowChart", function (data) {
            data.forEach(function (obj) {
                month.push(obj.Text);
                monthdata.push(obj.Value);
            })

            const ctx = document.getElementById('myChart');

            new Chart(ctx, {
                type: 'bar',
                data: {

                    labels: month,
                    datasets: [{

                        labels: '# Month Of Order',
                        data: monthdata,
                        borderColor: '#36A2EB',
                        backgroundColor: [
                            'rgba(255, 99, 132, 0.2)',
                            'rgba(255, 159, 64, 0.2)',
                            'rgba(255, 205, 86, 0.2)',
                            'rgba(75, 192, 192, 0.2)',
                            'rgba(54, 162, 235, 0.2)',
                            'rgba(153, 102, 255, 0.2)',
                            'rgba(201, 203, 207, 0.2)'
                        ],



                    }]
                },


            });


        });

    });

    $(document).ready(function () {
        var month = [];
        var monthdata = [];
        //var sid = $(this).val();
        // $("#CityId").empty();
        $.get("Seller/totalSaleseller", function (data) {
            data.forEach(function (obj) {
                month.push(obj.Text);
                monthdata.push(obj.Value);
            })

            const ctx = document.getElementById('Chart');

            new Chart(ctx, {
                type: 'bar',
                data: {

                    labels: month,
                    datasets: [{

                        labels: '# Month Of Order',
                        data: monthdata,
                        borderColor: '#36A2EB',
                        backgroundColor: [
                            'rgba(255, 99, 132, 0.2)',
                            'rgba(255, 159, 64, 0.2)',
                            'rgba(255, 205, 86, 0.2)',
                            'rgba(75, 192, 192, 0.2)',
                            'rgba(54, 162, 235, 0.2)',
                            'rgba(153, 102, 255, 0.2)',
                            'rgba(201, 203, 207, 0.2)'
                        ],



                    }]
                },


            });


        });

    });





</script>




